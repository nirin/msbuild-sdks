<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<Import Project="..\$(MSBuildThisFile)"/>

	<PropertyGroup>
		<BuildPath>$(SolutionDir)</BuildPath>
		<PublishPath>$(SolutionDir)</PublishPath>

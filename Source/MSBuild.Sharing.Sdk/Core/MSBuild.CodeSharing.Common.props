<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<Choose>
		<When Condition="'$(MSBuildProjectExtension)' == '.csproj'">
			<PropertyGroup>
				<LanguageName>CSharp</LanguageName>
				<LanguageSymbol>C#</LanguageSymbol>
				<LanguageExtension>.cs</LanguageExtension>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.vbproj'">
			<PropertyGroup>
				<LanguageName>VisualBasic</LanguageName>
				<LanguageSymbol>VB</LanguageSymbol>
				<LanguageExtension>.vb</LanguageExtension>
			</PropertyGroup>
		</When>

		<When Condition="'$(MSBuildProjectExtension)' == '.fsproj'">
			<PropertyGroup>
				<LanguageName>FSharp</LanguageName>
				<LanguageSymbol>F#</LanguageSymbol>
				<LanguageExtension>.fs</LanguageExtension>
			</PropertyGroup>
		</When>

		<Otherwise/>
	</Choose>

	<PropertyGroup Label="Globals">
		<MinimumVisualStudioVersion>14.0</MinimumVisualStudioVersion>
	</PropertyGroup>

	<PropertyGroup>
		<DefineCommonItemSchemas>true</DefineCommonItemSchemas>
		<TargetPlatformVersion Condition="'$(TargetPlatformVersion)' == ''">8.1</TargetPlatformVersion>
		<IsCodeSharingProject Condition="'$(IsCodeSharingProject)' == ''">true</IsCodeSharingProject>
	</PropertyGroup>

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>
	</PropertyGroup>

	<PropertyGroup>
		<CustomBeforeMicrosoftCommonProps>$(MSBuildThisFileDirectory)..\Build\MSBuild.NET.Workarounds.props</CustomBeforeMicrosoftCommonProps>
	</PropertyGroup>

	<Import Project="Sdk.props" Sdk="Microsoft.NET.Sdk"/>

	<Import Project="..\Build\MSBuild.NET.Extras.Sdk.props"/>
	<Import Project="..\ProjectSystem\MSBuild.NET.ProjectSystem.props"/>
	<Import Project="..\DefaultItems\MSBuild.NET.ImplicitPackages.props"/>

</Project>
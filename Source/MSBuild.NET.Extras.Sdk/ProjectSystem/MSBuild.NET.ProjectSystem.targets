<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup Condition="'$(MSBuildVersion)' == '' OR '$(MSBuildVersion)' &lt; '16.0'">
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<_SdkProjectSystemRulesDirectory>$(MSBuildThisFileDirectory)Rules\</_SdkProjectSystemRulesDirectory>
	</PropertyGroup>

	<PropertyGroup>
		<UsingLegacyProjectSystem Condition="'$(ProjectGuid)' != ''">true</UsingLegacyProjectSystem>
	</PropertyGroup>

	<PropertyGroup Condition="'$(UsingLegacyProjectSystem)' == 'true' AND '$(TargetFrameworkIdentifier)' == '.NETFramework'">
		<ProjectTypeGuids Condition="'$(EnableWpfProjectSetup)' == 'true'">$(ProjectTypeGuids);{60DC8134-EBA5-43B8-BCC9-BB4BC16C2548}</ProjectTypeGuids>
		<ProjectTypeGuids Condition="'$(EnableWffProjectSetup)' == 'true'">$(ProjectTypeGuids);{32F31D43-81CC-4C15-9DE6-3FC5453562B6}</ProjectTypeGuids>
		<ProjectTypeGuids Condition="'$(EnableWebProjectSetup)' == 'true'">$(ProjectTypeGuids);{349C5851-65DF-11DA-9384-00065B846F21}</ProjectTypeGuids>
		<ProjectTypeGuids Condition="'$(EnableWcfProjectSetup)' == 'true'">$(ProjectTypeGuids);{3D9AD99F-2412-4246-B90B-4EAA41C64699}</ProjectTypeGuids>
		<ProjectTypeGuids Condition="'$(EnableWinFormsProjectSetup)' == 'true'">$(ProjectTypeGuids);{}</ProjectTypeGuids>
	</PropertyGroup>

	<PropertyGroup Condition="'$(UsingLegacyProjectSystem)' == 'true'">
		<ProjectTypeGuids>$(DefaultProjectTypeGuid);$(ProjectTypeGuids)</ProjectTypeGuids>
		<ProjectTypeGuids>$(ProjectTypeGuids.Trim(';'))</ProjectTypeGuids>
	</PropertyGroup>

</Project>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup>
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup>
		<DefaultDesignerFileSuffix Condition="'$(DefaultDesignerFileSuffix)' == ''">.Designer</DefaultDesignerFileSuffix>
	</PropertyGroup>

	<ItemGroup Condition="'$(EnableWpfProjectSetup)' == 'true' AND '$(TargetFrameworkIdentifier)' == '.NETFramework'">
		<PropertyPageSchema Include="$(MSBuildThisFileDirectory)Rules\Wpf.ProjectItemsSchema.xml">
			<Context>Project</Context>
		</PropertyPageSchema>
		<PropertyPageSchema Include="$(MSBuildThisFileDirectory)Rules\ApplicationDefinition.xml">
			<Context>File;BrowseObject</Context>
		</PropertyPageSchema>
		<PropertyPageSchema Include="$(MSBuildThisFileDirectory)Rules\Resource.xml">
			<Context>File;BrowseObject</Context>
		</PropertyPageSchema>
		<PropertyPageSchema Include="$(MSBuildThisFileDirectory)Rules\Page.xml">
			<Context>File;BrowseObject</Context>
		</PropertyPageSchema>
	</ItemGroup>

	<PropertyGroup>
		<DefaultProjectTypeGuid Condition="'$(_LanguageSourceName)' == 'CSharp'">{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</DefaultProjectTypeGuid>
		<DefaultProjectTypeGuid Condition="'$(_LanguageSourceName)' == 'VisualBasic'">{F184B08F-C81C-45F6-A57F-5ABD9991F28F}</DefaultProjectTypeGuid>
		<!-- F# Legacy ProjectTypeGuid: {F2A71F9B-5D33-465A-A702-920D77279786} -->
		<DefaultProjectTypeGuid Condition="'$(_LanguageSourceName)' == 'FSharp'">{6EC3EE1D-3C4E-46DD-8F32-0CC8E7565705}</DefaultProjectTypeGuid>
	</PropertyGroup>

</Project>
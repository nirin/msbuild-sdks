<Project>

	<ImportGroup Condition="'$(SourceLanguageName)' != ''">
		<Import Project="..\Core\Languages\MSBuild.$(SourceLanguageName).props" Condition="'$(_ImportedSdkLanguageProps)' != 'true'"/>
		<Import Project="..\Core\Languages\MSBuild.$(SourceLanguageName).targets"/>
	</ImportGroup>

	<ImportGroup Condition="'$(_TargetFrameworkName)' != ''">
		<Import Project="..\Core\Frameworks\MSBuild.$(_TargetFrameworkName).props" Condition="'$(_ImportedSdkFrameworkProps)' != 'true'"/>
		<Import Project="..\Core\Frameworks\MSBuild.$(_TargetFrameworkName).targets"/>
	</ImportGroup>

	<ImportGroup Condition="'$(_TargetPlatformName)' != ''">
		<Import Project="..\Core\Platforms\MSBuild.$(_TargetPlatformName).props" Condition="'$(_ImportedSdkPlatformProps)' != 'true'"/>
		<Import Project="..\Core\Platforms\MSBuild.$(_TargetPlatformName).targets"/>
	</ImportGroup>

</Project>
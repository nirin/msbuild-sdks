<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<PropertyGroup Condition="'$(_ShortFrameworkProfile)' == 'client'">
		<TargetFrameworkProfile>Client</TargetFrameworkProfile>
		<TargetFrameworkVersion Condition="'$(TargetFrameworkVersion)' == ''">4.0</TargetFrameworkVersion>
		<IncludeProjectPriFile>false</IncludeProjectPriFile>
	</PropertyGroup>

	<PropertyGroup>
		<TargetFrameworkVersion Condition="'$(TargetFrameworkVersion)' == ''">4.5</TargetFrameworkVersion>
	</PropertyGroup>

	<PropertyGroup Condition="'$(MSBuildVersion)' == '' OR '$(MSBuildVersion)' &lt; '16.0'">
		<MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>
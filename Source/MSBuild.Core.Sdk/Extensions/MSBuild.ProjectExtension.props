<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<!-- We need to initialize 'MSBuildProjectExtensionsPath' property. -->
	<Import Project="MSBuild.ProjectExtension.Core.props" Condition="'$(ImportedMSBuildProjectExtensionCoreProps)' != 'true'"/>

	<PropertyGroup>
		<MSBuildProjectExtensionFileName Condition="'$(MSBuildProjectExtensionFileName)' == ''">$(MSBuildProjectFile).*</MSBuildProjectExtensionFileName>
	</PropertyGroup>

	<Import Project="$(MSBuildProjectExtensionsPath)$(MSBuildProjectExtensionFileName).props" Condition="Exists('$(MSBuildProjectExtensionsPath)')"/>

	<PropertyGroup Condition="'$(MSBuildVersion)' == '' OR '$(MSBuildVersion)' &lt; '16.0'">
		<MSBuildAllProjects Condition="Exists('$(MSBuildProjectExtensionsPath)')">$(MSBuildAllProjects);$(MSBuildProjectExtensionsPath)$(MSBuildProjectExtensionFileName).props</MSBuildAllProjects>
	</PropertyGroup>

	<PropertyGroup Condition="'$(MSBuildVersion)' == '' OR '$(MSBuildVersion)' &lt; '16.0'">
		<MSBuildAllProjects Condition="!Exists('$(MSBuildProjectExtensionsPath)')">$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
	</PropertyGroup>

</Project>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<Import Project="Microsoft.Managed.props"/>

	<!-- When we are doing a multi-targeting build, this import is redirected to $(MSBuildThisFileName).MultiTargeting.targets. -->
	<Choose>
		<When Condition="'$(IsOuterBuild)' == 'true'">
			<PropertyGroup>
				<MicrosoftCSharpTargets>$(MSBuildThisFileName).MultiTargeting.targets</MicrosoftCSharpTargets>
			</PropertyGroup>
		</When>
		<Otherwise>
			<PropertyGroup>
				<MicrosoftCSharpTargets>$(MSBuildThisFileName).CurrentVersion.targets</MicrosoftCSharpTargets>
			</PropertyGroup>
		</Otherwise>
	</Choose>

	<Import Project="$(MicrosoftCSharpTargets)"/>

	<Import Project="Microsoft.Managed.targets"/>

</Project>
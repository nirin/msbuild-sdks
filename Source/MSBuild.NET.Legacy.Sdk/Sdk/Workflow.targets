<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<Choose>
		<When Condition="'$(MSBuildAssemblyVersion)' == ''">
			<PropertyGroup>
				<WorkflowTargetsPath>$(MSBuildThisFileDirectory)Inbox\$(MSBuildThisFile)</WorkflowTargetsPath>
			</PropertyGroup>
		</When>
		<Otherwise>
			<PropertyGroup>
				<WorkflowTargetsPath>$(MSBuildThisFileDirectory)Current\$(MSBuildThisFile)</WorkflowTargetsPath>
			</PropertyGroup>
		</Otherwise>
	</Choose>

	<Import Project="$(MSBuildThisFileDirectory)Extensions\$(MSBuildThisFile)" Condition="'$(ImportLegacyExtensionTargets)' == 'true'"/>
	<Import Project="$(WorkflowTargetsPath)" Condition="'$(ImportLegacyExtensionTargets)' != 'true'"/>

</Project>
<Project>

	<Import Project="Microsoft.Managed.props"/>

	<!-- When we are doing a multi-targeting build, this import is redirected to $(MSBuildThisFileName).MultiTargeting.targets. -->
	<Choose>
		<When Condition="'$(IsOuterBuild)' == 'true'">
			<PropertyGroup>
				<MicrosoftFSharpTargets>$(MSBuildThisFileName).MultiTargeting.targets</MicrosoftFSharpTargets>
			</PropertyGroup>
		</When>
		<Otherwise>
			<PropertyGroup>
				<MicrosoftFSharpTargets>$(MSBuildThisFileName).CurrentVersion.targets</MicrosoftFSharpTargets>
			</PropertyGroup>
		</Otherwise>
	</Choose>

	<Import Project="$(MicrosoftFSharpTargets)"/>

	<Import Project="Microsoft.Managed.targets"/>

</Project>
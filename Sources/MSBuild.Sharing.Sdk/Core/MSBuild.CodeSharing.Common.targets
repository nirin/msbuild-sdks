<Project>

	<PropertyGroup>
		<_XamlLanguageTargets>$(MSBuildExtensionsPath)\Microsoft\WindowsXaml\v$(VisualStudioVersion)\Microsoft.Windows.UI.Xaml.$(LanguageSourceName).targets</_XamlLanguageTargets>
	</PropertyGroup>

	<Import Project="$(_XamlLanguageTargets)" Condition="Exists('$(_XamlLanguageTargets)')"/>
	<Import Sdk="MSBuild.NET.Legacy.Sdk" Project="Sdk.Core.targets" Condition="!Exists('$(_XamlLanguageTargets)')"/>

	<ItemGroup>
		<PropertyPageSchema Include="$(MSBuildToolsPath)\$(LangName)\SubProject.xaml"/>
	</ItemGroup>

	<ItemGroup>
		<ProjectCapability Include="SourceItemsFromImports;SharedImports;SharedAssetsProject"/>
		<ProjectCapability Include="PerPlatformCompilation;MultiTarget"/>
		<ProjectCapability Include="ProjectConfigurationsDeclaredAsItems"/>
		<ProjectCapability Include="HandlesOwnReload"/>
		<ProjectCapability Include="UseFileGlobs"/>
	</ItemGroup>

	<Target Name="Compile">
	</Target>

	<Target Name="Build">
		<ErrorFromResources Resource="CodeSharing.CannotBuildSharedProject"/>
	</Target>

	<Target Name="Clean">
		<ErrorFromResources Resource="CodeSharing.CannotBuildSharedProject"/>
	</Target>

	<Target Name="_CheckForInvalidConfigurationAndPlatform">
	</Target>

</Project>
<Project>

	<!-- This file redirects to all the tasks that ship by default with MSBuild within .NET Framework in Windows -->

	<Import Project="$(MSBuildFrameworkToolsPath)Microsoft.Common.Tasks"/>
	<Import Project="$(MSBuildFrameworkToolsPath)Microsoft.Common.OverrideTasks" Condition="Exists('$(MSBuildFrameworkToolsPath)Microsoft.Common.OverrideTasks')"/>

	<PropertyGroup>
		<ImportedMicrosoftCommonTasks>true</ImportedMicrosoftCommonTasks>
	</PropertyGroup>

</Project>